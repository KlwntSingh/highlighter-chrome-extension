{
    "name": "kulwant extension",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["tabs", "activeTab", "contextMenus", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["jquery.js"]
        }
    ],
    "manifest_version": 2
}
