{
    "name": "highlighter",
    "version": "1.0",
    "description": "highlighting the text in webpage for session",
    "permissions": ["tabs", "activeTab", "contextMenus"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["website/content.css"],
          "js": ["website/jquery.js", "website/content.js"]
        }
    ],
    "manifest_version": 2
}